<% include ../partials/header %>

<div class="container">
	<% thisUser.supplyPosts.forEach(function(supply){ %>

		<div class="card" style="width: 18rem;">
			<div class="_cardImgBox">
				<img class="card-img-top" src="..." alt="Do u know the way">
			</div>
		  <div class="card-body">
		    <h6 class="card-title"><%= supply.s_unit%> unit(s) available</h6>
		    <p class="card-text"><%= supply.s_description%></p>
		    <a href="#" class="btn btn-primary">Detail</a>
		    <a href="#" class="btn btn-secondary">Edit</a>
		  </div>
		</div>

	<% }); %>
</div>

<% include ../partials/footer %>