<!-- supplySet -->
<% include ../partials/header %>
	
	<div class="container">
		
		<h1 class="_textCenter jumbotron">Uzespace Market[BETA]</h1>
		<p class="_note" style="font-style: italic;">"When sharing the space, technically you made uzers' storage experience great again..."  —— A uzespace cofounder</p>

		<div class="_cardComplex">
			<% supplySet.forEach(function(supply) { %>
	    	<div class="card _floatedL" style="width: 288px; margin: 6px; margin-bottom: 10px;">
					<div class="_cardImgBox">
						<!-- /img/logo2.png -->
						<img class="card-img-top" style="height: 100%;object-fit: cover;" src="<%=supply.s_photoUrl%>" alt="Do u know the way" onerror="this.onerror=null;this.src='/img/logo2.png';">
					</div>
				  <div class="card-body">
				    <h6 class="card-title"><%= supply.s_unit%> unit(s) available</h6>
				    <p class="card-text"><%= supply.s_description%></p>

						<% var s = supply.s_college; %>
						<% s = s.substring(0, s.indexOf('(')); %>
				    <p style="font-size: 0.8em;"><%=s %>: <%=supply.s_starting%> -> <%=supply.s_ending%></p>
				    <a href="/market/supplyshow/<%=supply._id%>" class="btn btn-primary">Detail</a>
				  </div>
				</div>
	      <!-- userMap[user._id] = user; -->
	    <% }); %>

	    <div class="_clearFloat"></div>
		</div>

    

    <div class="_clearFloat"></div>
	</div>



<% include ../partials/footer %>